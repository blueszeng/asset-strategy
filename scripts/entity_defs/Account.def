<root>
	<Volatile>
		<position/>
		<!--<position> 0 </position> Don't update-->
		<yaw/>
		<!--<pitch> 20 </pitch>-->
		<pitch/>
		<roll/>
	</Volatile>
	<Properties>
		<WarFieldId>
			<Type>	INT32		</Type>
			<Flags>	CELL_PRIVATE	</Flags>
			<Default>	-1	</Default>
			<Persistent>	false	</Persistent>
		</WarFieldId>
	</Properties>

	<ClientMethods>
		<cellReady>
			<Arg>		INT8		</Arg><!--游戏模式-->
		</cellReady>
		<addNewUnit>
			<Arg>		INT8		</Arg>
			<Arg>		INT16		</Arg>
			<Arg>		SkillList	</Arg>
			<Arg>		FLOAT		</Arg>
			<Arg>		FLOAT		</Arg>
			<Arg>		INT64		</Arg>
		</addNewUnit>
		<int64>
			<Arg>	SkillList		</Arg>
		</int64>
		<setMoving>
			<Arg>		INT8		</Arg>
		</setMoving>
		<setSpeed>
			<Arg>		FLOAT		</Arg>
		</setSpeed>
		<setDirect>
			<Arg>		VECTOR2		</Arg>
		</setDirect>
		<setShift>
			<Arg>		VECTOR2		</Arg>
		</setShift>
		<turnNo>
			<Arg>		INT8		</Arg>
		</turnNo>
		<updateEnd>
			<Arg>		INT32		</Arg>
		</updateEnd>
		<takeDamage>
			<Arg>		INT8		</Arg>
		</takeDamage>
		<useSkill>
			<Arg>		INT8		</Arg>
			<Arg>		INT8		</Arg>
		</useSkill>
		<useSkillmulti><!--用於施放多個角色的技能-->
			<Arg>		INT8		</Arg>
			<Arg>		SkillList		</Arg>
			<!--SkillList用於傳目標角色編號列表-->
		</useSkillmulti>
		<beTreat>
			<Arg>		INT8		</Arg>
		</beTreat>
		<beRepel>
			<Arg>		VECTOR2		</Arg>
			<Arg>		FLOAT		</Arg>
		</beRepel>
		<addBuff>
			<Arg>		INT8		</Arg>
		</addBuff>
		<delBuff>
			<Arg>		INT8		</Arg>
		</delBuff>
		<died>
		</died>
		<setcanMove>
			<Arg>		INT8		</Arg>
		</setcanMove>
		<createEffection>
			<Arg>		INT16		</Arg>
			<Arg>		INT8		</Arg>
		</createEffection>
		<addTrap>
			<Arg>		INT16		</Arg>
			<Arg>		INT8		</Arg>
			<Arg>		VECTOR2		</Arg>
			<Arg>		INT32		</Arg>
		</addTrap>
		<delTrap>
			<Arg>		INT8		</Arg>
		</delTrap>
		<roundBegin>
			<Arg>		INT32		</Arg>
		</roundBegin>
	</ClientMethods>

	<BaseMethods>
		<createSpace>
			<Exposed/>
		</createSpace>
		<startDebugMode>
			<Exposed/>
		</startDebugMode>
		<startMatching>
			<Exposed/>
		</startMatching>
	</BaseMethods>

	<CellMethods>
		<move>
			<Exposed/>
			<Arg>		VECTOR2		</Arg>
		</move>
		<createRole>
			<Exposed/>
			<Arg>		INT8		</Arg>
			<Arg>		VECTOR2		</Arg>
		</createRole>
		<debugGame>
			<Exposed/>
		</debugGame>
		<debugTeam>
			<Exposed/>
		</debugTeam>
		<createTrap>
			<Exposed/>
			<Arg>		INT16		</Arg>
			<Arg>		VECTOR2		</Arg>
		</createTrap>
		<clientloadingReady>
			<Exposed/>
		</clientloadingReady>
